<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Base/MenuItemBase.xaml" />
        <ResourceDictionary Source="pack://application:,,,/QR.Shell;component/Resources/Brushes.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <Style
        x:Key="MenuItemDefault"
        BasedOn="{StaticResource MenuItemBase}"
        TargetType="MenuItem" />


    <Style
        x:Key="MenuItemBlank"
        BasedOn="{StaticResource BlankMenuItemBase}"
        TargetType="MenuItem" />

    <Style
        x:Key="MenuItemPure"
        BasedOn="{StaticResource PureMenuItemBase}"
        TargetType="MenuItem" />

</ResourceDictionary>